<?php


namespace AppBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;

/**
 * Class Torrent
 * @package AppBundle\Model
 * @MongoDB\Document
 */
class Torrent {

    /**
     * @MongoDB\Id
     */
    protected $id;

    /**
     * @MongoDB\String
     */
    protected $slug;

    /**
     * @MongoDB\String
     */
    protected $title;

    /**
     * @MongoDB\String
     */
    protected $description;

    /**
     * @MongoDB\String
     */
    protected $downloadLink;

    /**
     * @MongoDB\String
     */
    protected $size;

    /**
     * @MongoDB\Int
     */
    protected $seeds;

    /**
     * @MongoDB\Int
     */
    protected $leechs;

    /**
     * @MongoDB\String
     */
    protected $url;

    /**
     * @MongoDB\String
     */
    protected $tracker;

    /**
     * @MongoDB\String
     */
    protected $category;

} 